<GridContainer Column="0" Visible="false" FillColor="@MidGrey" BorderWidth="1" Name="MapSettings">
	<Rows>
		<Row Height="28" ID="Title"/>
		<Row Height="1"/>
		<Row Height="16"/>
		<Row Height="24" ID="MapName"/>
		<Row Height="16"/>
		<Row Height="24" ID="MapType"/>
		<Row Height="2"/>
		<Row Height="24" ID="Origin"/>
		<Row Height="16"/>
		<Row Height="2" ID="Separator"/>
		<Row Height="10"/>
		<Row Height="24" ID="GameModes"/>
	</Rows>
	<Image RowID="Title" Image="while.tga" ColorMask="@DarkGrey"/>
	<Text RowID="Title" Text="Map Settings" Style="InvertedText"/>
	<Image Row="1" ColorMask="#000000"/>

	<GridContainer RowID="MapName">
		<Columns>
			<Column Width="10"/>
			<Column Width="1*"/>
			<Column Width="10"/>
		</Columns>
		<EditBox Column="1" Text="Map name" Style="NormalText" HighlightColor="#777777" Name="MapName"/>
	</GridContainer>

	<GridContainer RowID="MapType">
		<Columns>
			<Column Width="10"/>
			<Column Width="64"/>
			<Column Width="1*"/>
			<Column Width="10"/>
		</Columns>
		<Text Column="1" Text="Map type:" TextAlignH="Left" FontType="@BasicFont" Color="#333333" FontSize="9"/>
		<ComboBox Column="2" Style="NormalText" HighlightColor="#777777" Name="MapType"/>
	</GridContainer>
	<GridContainer RowID="Origin">
		<Columns>
			<Column Width="10"/>
			<Column Width="64"/>
			<Column Width="1*"/>
			<Column Width="10"/>
		</Columns>
		<Text Column="1" Text="Origin:" TextAlignH="Left" FontType="@BasicFont" Color="#333333" FontSize="9"/>
		<ComboBox Column="2" Style="NormalText" HighlightColor="#777777" Name="Origin"/>
	</GridContainer>
	<Image RowID="Separator" ColorMask="#000000"/>
	<Text RowID="GameModes" Text="Game Modes:" Style="NormalText"/>
</GridContainer>