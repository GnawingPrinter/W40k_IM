		<Icon Width="@IconSize" MarginLeft="2" MarginRight="2" AllImg="white.tga" Text="Test" Style="NormalText" Name="Test"/>
		<Icon Width="@IconSize" MarginLeft="2" MarginRight="2" AllImg="white.tga" Text="Mosaic" Style="NormalText" Name="Mosaic"/>
