			<Rows>
				<Row Height="30"/>
				<Row Height="30"/>
				<Row Height="30"/>
				<Row Height="30"/>
			</Rows>
			<Columns>
				<Column Width="200"/>
				<Column Width="1*"/>
			</Columns>
			
			<Text Row="0" Column="0" MarginRight="10" TextAlignH="right">Critical Streak</Text>
			<Text Row="0" Column="1" MarginLeft="10" TextAlignH="left" Name="AngelOfDeath_streak" Text="0"/>
			
