<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Window>
	<StyleRules>
		<Rule Pattern="Text" FontSize="14" Color="#ffff00"/>
	</StyleRules>

	<FlowContainer Width="500" HAlign="right" VAlign="bottom" FillColor="#232323" Margin="10">
		<Container Height="30">
			<Icon Text="Killstreak" Color="#ffffff" ColorMask="#545454" FontSize="16" Name="KillstreakIcon"/>
		</Container>
		<GridContainer BorderWidth="1" BorderColor="#ffffff" Name="KillstreakGrid" Visible="false">
			<Rows>
				<Row Height="30"/>
				<Row Height="30"/>
				<Row Height="30"/>
				<Row Height="30"/>
				<Row Height="30"/>
				<Row Height="30"/>
			</Rows>
			<Columns>
				<Column Width="200"/>
				<Column Width="1*"/>
			</Columns>
			
			<Text Row="0" Column="0" MarginRight="10" TextAlignH="right">Streak Index</Text>
			<Text Row="0" Column="1" MarginLeft="10" TextAlignH="left" Name="Killstreak_streakindex" Text="0"/>
			
			<Text Row="1" Column="0" MarginRight="10" TextAlignH="right">Current Kill Count</Text>
			<Text Row="1" Column="1" MarginLeft="10" TextAlignH="left" Name="Killstreak_killcount" Text="6"/>
			
			<Text Row="2" Column="0" MarginRight="10" TextAlignH="right">Time Left To Kill</Text>
			<Text Row="2" Column="1" MarginLeft="10" TextAlignH="left" Name="Killstreak_timeleft" Text="6"/>
			
			<Text Row="3" Column="0" MarginRight="10" TextAlignH="right">Xp For Current Streak</Text>
			<Text Row="3" Column="1" MarginLeft="10" TextAlignH="left" Name="Killstreak_xpforstreak" Text="6"/>
			
			<Text Row="4" Column="0" MarginRight="10" TextAlignH="right">Buff Magic ID</Text>
			<Text Row="4" Column="1" MarginLeft="10" TextAlignH="left" Name="Killstreak_magicid" Text="6"/>
			
			<Text Row="5" Column="0" MarginRight="10" TextAlignH="right">Buff Duration Left</Text>
			<Text Row="5" Column="1" MarginLeft="10" TextAlignH="left" Name="Killstreak_magicduration" Text="6"/>
		</GridContainer>
		
		
		<Container Height="30">
			<Icon Text="AngelOfDeath" Color="#ffffff" ColorMask="#545454" FontSize="16" Name="AngelOfDeathIcon"/>
		</Container>
