	<!-- NAMED POSITIONS SUBWINDOW -->
	<Container VAlign="Center" HAlign="Center" ElementDirectory="Editor" Name="NamedPosSubWindow" Visible="0">
		<Image ColorMask="#555555"/>
		<FlowContainer>
			<Container Width="500" Height="40">
				<Text Text="Named Position"		Style="TextTitle"    Name="NamedPosTxt_Name"	MarginTop="5" MarginLeft="5" />
				<Text Text="X"                 	Style="TextTitle"    Name="NamedPosTxt_x"     	MarginTop="5" MarginLeft="250" />
				<Text Text="Y"                 	Style="TextTitle"    Name="NamedPosTxt_y"     	MarginTop="5" MarginLeft="350" />
				<Text Text="Z"                 	Style="TextTitle"    Name="NamedPosTxt_z"     	MarginTop="5" MarginLeft="450" />
			</Container>
			<Loop Count="@NamedPosDisplayLimit" Variable="$X$">
				<FlowContainer Horizontal="1" Height="40" Margin="5">
					<EditBox Style="TextTitle" MarginLeft="10"  MarginRight="10" Name="NamedPositionName_$X$" Width="200" Graphics="UI/Element/Editor/EDITBOX" />
                    <EditBox Style="TextTitle" MarginLeft="10"  MarginRight="10" Name="NamedPositionX_$X$" Width="80" Graphics="UI/Element/Editor/EDITBOX" />
                    <EditBox Style="TextTitle" MarginLeft="10"  MarginRight="10" Name="NamedPositionY_$X$" Width="80" Graphics="UI/Element/Editor/EDITBOX" />
                    <EditBox Style="TextTitle" MarginLeft="10"  MarginRight="10" Name="NamedPositionZ_$X$" Width="80" Graphics="UI/Element/Editor/EDITBOX" />
					<CheckBox Style="TextTitle" MarginLeft="6" MarginRight="6"  Name="NamedPositionShow_$X$" Width="50" ColorMask="#FF0000" Tooltip="show this position" />	
					<Icon Size="30,30" ColorMask="#880000" Text="-" MarginLeft="5" VAlign="Center" Name="DeleteNamedPosEntry_$X$"/>
				</FlowContainer>
			</Loop>
			<Icon Size="120, 40" ColorMask="#333333" Text="Ok" HAlign="Center" Name="NamedPosWindOk"/>
		</FlowContainer>		
	</Container>	
	
	<!-- SKILLS SUBWINDOW -->
	<Container VAlign="Center" HAlign="Center" MarginLeft="280" ElementDirectory="Editor" Name="SkillsSubWindow" Visible="0">
		<Image ColorMask="#555555"/>
		<FlowContainer>
			<Container Width="740" Height="40">
				<Text Text="Skill name"					Style="TextTitle"    Name="SkillTxt_Name"			MarginTop="5" MarginLeft="5" />
				<Text Text="ON"                 		Style="TextTitle"    Name="SkillsTxt_enabled"     	MarginTop="5" MarginLeft="440" />
				<Text Text="Prio"                 		Style="TextTitle"    Name="SkillsTxt_priority"     	MarginTop="5" MarginLeft="500" />
				<Text Text="Bodyregion"                 Style="TextTitle"    Name="SkillsTxt_bodyregion"    MarginTop="5" MarginLeft="550" />
				<Text Text="OtherParams"              	Style="TextTitle"    Name="SkillsTxt_otherparams"   MarginTop="5" MarginLeft="670" />
			</Container>
			<Loop Count="@SkillsDisplayLimit" Variable="$X$">
				<FlowContainer Horizontal="1" Height="40" Margin="5">
					<EditBox Style="TextTitle" MarginLeft="6"  MarginRight="6" Name="SkillName_$X$" Width="420" Graphics="UI/Element/Editor/EDITBOX" />
					<CheckBox Style="TextTitle" MarginLeft="6" MarginRight="6"  Name="SkillEnabled_$X$" Width="50" ColorMask="#FF0000" Tooltip="enable or disable that is the question" />	
					<EditBox Style="TextTitle" MarginLeft="6"  MarginRight="6" Name="SkillPriority_$X$" Width="30" Graphics="UI/Element/Editor/EDITBOX" Tooltip="value of how likely the unit will use it when goverened by ai (higher = more likely)"  />
					<ComboBox Style="TextTitle" MarginLeft="6"  MarginRight="6" Name="SkillBodyRegion_$X$" Width="100" FontSize="18" HighlightColor="#777777" />
					<EditBox Style="TextTitle" MarginLeft="6"  MarginRight="6" Name="SkillOtherParam_$X$" Width="300" Graphics="UI/Element/Editor/EDITBOX" Tooltip="other params, such as animpack/level/actionslot"  />
					<Icon Size="30,30" ColorMask="#880000" Text="-" MarginLeft="5" VAlign="Center" Name="DeleteSkillEntry_$X$"/>
				</FlowContainer>
			</Loop>
			<Icon MarginLeft="10" Width="60" Height="60" ColorMask="#008800" Text="+" Name="AddSkillEntry"/>
			<Icon Size="120, 40" ColorMask="#333333" Text="Ok" HAlign="Center" Name="SkillWindOk"/>
		</FlowContainer>		
	</Container>	
	
